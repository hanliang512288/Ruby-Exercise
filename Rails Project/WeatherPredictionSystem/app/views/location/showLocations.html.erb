<h1>Locations</h1>
<p>{</p>
	<p style="text-indent: 1cm">"date":<%= @date%></p>
	<p style="text-indent: 1cm">"locations":[</p>
    <% @locations["locations"].each do |l| %>
    <p style="text-indent: 1.5cm">{</p>
    <p style="text-indent: 2cm">"id": <%= l['id']%></p>
    <p style="text-indent: 2cm">"lat": <%= l['lat']%></p>
    <p style="text-indent: 2cm">"lon": <%= l['lon']%></p>
    <p style="text-indent: 2cm">"last_update": <%= l['last_update']%></p>
    <p style="text-indent: 1.5cm">}</p>
    <% end %>
	<p style="text-indent: 1cm">]</p>
<p>}</p>


